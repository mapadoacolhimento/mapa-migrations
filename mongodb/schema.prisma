generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
  binaryTargets   = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_DB_URL")
}

model FormData {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String
}